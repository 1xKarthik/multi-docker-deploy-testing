{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "karthikgirraj/resume-aws-app:v3",
            "hostname": "app",
            "essential": false,
            "memory": 128
        },
        {
            "name": "db",
            "image": "karthikgirraj/resume-aws-db:v2",
            "hostname": "db",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "karthikgirraj/resume-aws-server",
            "hostname": "server",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["app", "db"]
        }
    ]
}